{
  "name": "start-template",
  "version": "1.0.1",
  "scripts": {
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node node_scripts/build-assets.js",
    "build:pug": "node node_scripts/build-pug.js",
    "build:scripts": "node node_scripts/build-scripts.js",
    "build:scss": "node node_scripts/build-scss.js",
    "clean": "node node_scripts/clean.js",
    "start": "npm run build && node node_scripts/start.js",
    "start:debug": "npm run build && node node_scripts/start-debug.js"
  },
  "description": "start html/bootstrap template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidkolisek/start_template.git"
  },
  "author": "davidkolisek",
  "bugs": {
    "url": "https://github.com/davidkolisek/start_template/issues"
  },
  "homepage": "https://github.com/davidkolisek/start_template#readme",
  "dependencies": {
    "bootstrap": "5.2.2",
    "jquery": "^3.6.1",
    "jquery.cookie": "^1.4.1"
  },
  "devDependencies": {
    "autoprefixer": "10.4.12",
    "browser-sync": "2.27.10",
    "browserify": "^17.0.0",
    "chokidar": "3.5.3",
    "concurrently": "7.4.0",
    "css-minify": "^2.0.0",
    "esmify": "^2.1.1",
    "exorcist": "^2.0.0",
    "postcss": "8.4.31",
    "prettier": "2.7.1",
    "pug": "3.0.2",
    "sass": "1.55.0",
    "shelljs": "0.8.5",
    "uglify-js": "^3.17.2",
    "upath": "2.0.1"
  }
}
